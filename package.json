{
  "name": "ts-node-postgres-starter",
  "version": "1.0.0",
  "description": "RESTful web service template using Node, TypeScript, and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "nodemon --exec \"ts-node\" src/server.ts",
    "debug": "nodemon --exec \"node --inspect -r ts-node/register\" src/server.ts",
    "build": "tsc",
    "test": "jest --testPathIgnorePatterns \"/(integration-tests)/\"",
    "test:coverage": "yarn test --coverage",
    "test:watch": "yarn test --watch",
    "test:int": "run-s test:int:setup test:int:run",
    "test:int:setup": "NODE_ENV=test yarn run knex migrate:latest",
    "test:int:run": "jest --runInBand --testMatch \"**/integration-tests/**/*.ts\"",
    "test:ci": "run-s \"test --runInBand\" test:int",
    "coverage-report": "node ./scripts/open-coverage-report",
    "lint": "eslint --ext .ts src/",
    "prettier": "prettier --write **/*.ts",
    "prettier:check": "prettier --check **/*.ts",
    "ci:check": "run-s prettier:check lint test:ci",
    "knex": "knex"
  },
  "author": "Matthew Laufer",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^4.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.17",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.4.1",
    "redis": "^2.8.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^16.0.9",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^25.1.1",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.7.0",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.14",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "knex": "^0.20.8",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
