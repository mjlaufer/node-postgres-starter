{
  "name": "ts-node-pgp-starter",
  "version": "1.0.0",
  "description": "TypeScript-Node API starter project",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server/index.ts",
    "dev": "nodemon --exec \"ts-node\" src/server/index.ts",
    "debug": "nodemon --exec \"node --inspect -r ts-node/register\" src/server/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand",
    "coverage-report": "node ./scripts/open-coverage-report",
    "lint": "eslint --ext .ts src/",
    "prettier": "prettier --write **/*.ts",
    "prettier:check": "prettier --check **/*.ts",
    "ci:check": "run-s prettier:check lint test:ci"
  },
  "author": "Matthew Laufer",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.3.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.0.1",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^16.0.4",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.14",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  }
}
