{
  "name": "ts-node-postgres-starter",
  "version": "1.0.0",
  "description": "RESTful web service template using Node, TypeScript, and PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"ts-node\" src/server.ts",
    "debug": "nodemon --exec \"node --inspect -r ts-node/register\" src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand",
    "coverage-report": "node ./scripts/open-coverage-report",
    "lint": "eslint --ext .ts src/",
    "prettier": "prettier --write **/*.ts",
    "prettier:check": "prettier --check **/*.ts",
    "ci:check": "run-s prettier:check lint test:ci",
    "knex": "knex"
  },
  "author": "Matthew Laufer",
  "license": "MIT",
  "dependencies": {
    "@hapi/joi": "^17.0.2",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^4.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.3.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pg-promise": "^10.3.3",
    "redis": "^2.8.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/hapi__joi": "^16.0.6",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.1.6",
    "@types/passport": "^1.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/redis": "^2.8.14",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "knex": "^0.20.7",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  }
}
