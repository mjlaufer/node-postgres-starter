{
  "name": "ts-node-pgp-starter",
  "version": "1.0.0",
  "description": "TypeScript-Node API starter project",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon --exec \"ts-node\" src/server.ts",
    "debug": "nodemon --exec \"node --inspect -r ts-node/register\" src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --runInBand",
    "coverage-report": "node ./scripts/open-coverage-report",
    "lint": "eslint --ext **/**.ts",
    "prettier": "prettier --write --only-changed **/**.ts",
    "prettier:check": "prettier --check **/**.ts",
    "ci:check": "run-s prettier:check lint test:ci"
  },
  "author": "Matthew Laufer",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "pg-promise": "^9.3.5"
  },
  "devDependencies": {
    "@types/compression": "^1.0.1",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/jest": "^24.0.22",
    "@types/lodash": "^4.14.145",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "open": "^7.0.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.2"
  }
}
